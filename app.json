{
  "expo": {
    "name": "Iskonnect",
    "slug": "iskonnect",
    "scheme": "iskonnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/adaptive.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sakyo11.iskonnect"
    },
    "android": {
      "package": "com.sakyo11.iskonnect",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-video"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d568077b-a21d-4004-82d4-0faadf53aebb"
      }
    },
    "newArchEnabled": true
  }
}
